/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/* 其他的设置 */
page,view,text,swiper,swiper-item,image,navigator{
  padding:0px;
  margin:0px;
box-sizing:border-box
}
/* 主题颜色 通过变量来实现    定义主题颜色*/
page{
--themeColor:#008000;
font-size: 28rpx ;
}

image{
  width:100%;
}