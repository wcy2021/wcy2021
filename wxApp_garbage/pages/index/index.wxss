/* swiper 样式 */
.index_swiper swiper {
    width: 750rpx;
    height: 340rpx;
  }
.index_swiper swiper image {
    width: 100%;
  }
/* icon样式 */
  .icon_all{
      display: flex;
  }
  .icon_all navigator {
    padding: 20rpx;
    flex: 1;
  }
  .icon_all navigator image {
    width: 100%;
  }

  .icon_all navigator image:nth-child(1){
    margin-top: 30rpx;
  }
  /* floor 样式 */
  index_floor .floor_group .floor_title image {
    width: 100%;
  }
  .index_floor .floor_group .floor_list navigator {
    float: left;
    width: 33.33%;
  }
  .index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
    height:27.7vw;

  }
  .index_floor .floor_group .floor_list navigator image {
    width: 100%;
    height: 100%;
  }

  /* 补充样式 */
  .index_floor .floor_group .floor_list navigator:nth-child(4) image{
   margin-top: -72rpx;
  }
  .index_floor .floor_group .floor_list navigator:nth-child(5) image{
    margin-top: -22rpx;
   }
   .index_floor .floor_group .floor_list navigator:nth-child(6) image{
    margin-top: -42rpx;
   }
 
  .floor_title{
      font-size: 40rpx;
      color:var(--themeColor);
  }